{
	"extends": [
		"airbnb",
		"prettier",
		"plugin:react/jsx-runtime"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"no-undef": 1,
		"react/prop-types": 0,
		"react/jsx-filename-extension": 0,
		"react/prefer-stateless-function": 1,
		"import/prefer-default-export": 0,
		"no-unused-vars": 0
	}
}